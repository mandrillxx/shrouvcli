  "description": "",
  "main": "index.js",
  "scripts": {
    "build": "rbxtsc",
    "compile": "rbxtsc --verbose",
    "watch": "rbxtsc -w",
    "tarmac": "tarmac sync --target roblox",
    "build:dev": "rmdir out /s && yarn compile && rojo build --output game.rbxlx",
    "build:prod": "rmdir out /s && cross-env NODE_ENV=production yarn compile && rojo build --output game.rbxlx",
    "deploy:dev": "yarn build:dev && mantle deploy --environment dev",
    "deploy:prod": "yarn build:prod && mantle deploy --environment production",
    "output:dev": "mantle outputs --environment dev > mantle-output.json",
    "output:prod": "mantle outputs --environment production > mantle-output.json"
  },
